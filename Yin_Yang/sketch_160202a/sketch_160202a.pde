// create a canvas
size(400, 400);
fill(#FF038E);
noStroke ();
ellipse(200,200,1000,1000);
fill(#FF03E6);
noStroke ();
ellipse(200,200,500,500);
fill(#A603FF);
noStroke ();
ellipse(200,200,400,400);
fill(#1603FF);
noStroke ();
ellipse(200,200,300,300);
fill (255);
noStroke ();
ellipse(200,200,200,200);
fill(#0376FF);
arc(200,200,200,200, radians(90), radians(270));
fill(255);
ellipse(200,150,100,100);

fill (#0376FF);
ellipse(200,150,30,30);
noStroke ();
ellipse(200,250,100,100);
fill (255);
ellipse(200,250,30,30);